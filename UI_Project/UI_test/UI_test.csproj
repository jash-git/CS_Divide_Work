	<FileVersion>1.0.0.1</FileVersion>
	<BaseOutputPath>..\..\All_Bin\UI_test</BaseOutputPath>
	<!-- 版本編號 & 編譯結果輸出路徑 end -->
	
  </PropertyGroup>

  <ItemGroup>
