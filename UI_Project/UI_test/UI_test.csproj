	  <!--<HintPath>..\..\All_Bin\LogFile\x86\Release\net6.0\LogFile.dll</HintPath> -->
    </Reference>
    <Reference Include="WinForms_Share">
	  <HintPath>..\..\All_Bin\WinForms_Share\x86\Debug\net6.0-windows\WinForms_Share.dll</HintPath>
	  <!--<HintPath>..\..\All_Bin\WinForms_Share\x86\Release\net6.0-windows\WinForms_Share.dll</HintPath>-->	
    </Reference>
    <Reference Include="Dy_Dll">
	  <HintPath>..\..\All_Bin\Dy_Dll\x86\Debug\net6.0\Dy_Dll.dll</HintPath>
	  <!--<HintPath>..\..\All_Bin\Dy_Dll\x86\Release\net6.0\Dy_Dll.dll</HintPath>-->	
